<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.RootLayoutController">
<top><MenuBar BorderPane.alignment="CENTER">
  <menus>
    <Menu mnemonicParsing="false" text="Arquivo">
      <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleNew" text="Novo" />
                  <MenuItem mnemonicParsing="false" onAction="#handleOpen" text="Abrir" />
                  <MenuItem mnemonicParsing="false" onAction="#handleSave" text="Salvar" />
                  <MenuItem mnemonicParsing="false" onAction="#handleSaveAs" text="Salvar como" />
        <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Sair" />
      </items>
    </Menu>
    <Menu fx:id="BarChart" mnemonicParsing="false" text="Estatisticas">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#handleShowBirthdayStatistics" text="Mostrar estatisticas" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Help">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#handleAbout" text="About" />
      </items>
    </Menu>
  </menus>
</MenuBar>
</top></BorderPane>
